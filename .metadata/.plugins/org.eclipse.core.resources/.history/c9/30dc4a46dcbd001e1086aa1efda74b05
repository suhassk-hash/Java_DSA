package SK_DSA.Sort;

public class Insertion_Sort {
public static void main(String[]args) {
	int arr []= {2,3,1,4,5,62,11,65,7,1,2,3,4,5632};
	sort(arr);
	sysout
}
static void sort(int [] arr) {
	for(int i=0;i<=arr.length-2;i++) {
		int j=i+1;
		while(j>0) {
			if(arr[i]>arr[j]) {
				int temp=arr[j+1];
				arr[j+1]=arr[j];
				arr[j]=temp;
				j--;
			}
			else {
				break;
			}
		}
	}
}
}
