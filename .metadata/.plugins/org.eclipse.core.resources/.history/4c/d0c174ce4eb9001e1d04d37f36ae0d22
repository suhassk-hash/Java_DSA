	package SK_DSA.BInary_Search;

public class Infinite_Array {
	public static void search(int []arr,int target) {
		int start=0,end=1;
		while(target>end) {
			int nstart=end+1;
			end=end+(end-start+1)*2;		
		}
		B
	}
	public static int Binary__Search(int []arr,int target,int start,int end) {
			int middle=start+(end-start/2);

				if(target<arr[middle]) {//in case of descending ordered array
					end=middle-1;		//changes to start=middle+1;
				}
				else if(target>arr[middle]) {
					start=middle+1;		//changes to end=middle-1;
				}
				else if(target==arr[middle]) {
					return middle;
				}
				return -1;
	}
}
